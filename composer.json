{
    "name": "fm-labs/cakephp-mailman",
    "description": "Mailman plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Mailman\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mailman\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
